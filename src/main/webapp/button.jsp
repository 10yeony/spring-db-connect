<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<style>
.button {
  font-family: "Roboto", sans-serif;
  outline: none;
  height: 30px;
  text-align: center;
  width: 70px;
  border-radius: 40px;
  background: #fff;
  border: 2px solid #416dea;
  color: #416dea;
  letter-spacing: 1px;
  text-shadow: 0;
  font-weight: bold;
  cursor: pointer;
  transition: all 0.25s ease;
}
.button:hover {
  color: white;
  background: #416dea;
}
.button:active {
  letter-spacing: 2px;
}

</style>
</head>
<body>
<div>
  <button class="button">가입승인</button>
</div>
</body>
</html>